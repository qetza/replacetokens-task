{
  "manifestVersion": 1,
  "id": "replacetokens",
  "name": "Replace Tokens",
  "version": "5.0.0",
  "public": true,
  "publisher": "qetza",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Task to replace tokens in files.",
  "categories": [
    "Build and release"
  ],
  "tags": [
    "release",
    "build",
    "token",
    "replace",
    "variable",
    "substitution"
  ],
  "links": {
    "support": {
      "uri": "https://github.com/qetza/replacetokens-task/issues"
    }
  },
  "icons": {
    "default": "images/icon.png"
  },
  "branding": {
    "color": "#6F42C1",
    "theme": "dark"
  },
  "screenshots": [
    {
      "path": "images/screenshot01.png"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/qetza/replacetokens-task"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "files": [
    {
      "path": "tasks"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "replacetokens-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks"
      }
    }
  ]
}